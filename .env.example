# Copy to .env and fill with your credentials
ROAM_GRAPH_NAME="your-graph"
ROAM_API_TOKEN="roam-graph-token-xxxxx"

# Optional provider keys
GOOGLE_API_KEY=""
VOYAGEAI_API_KEY=""

# Service endpoints
WEAVIATE_HTTP_HOST=weaviate
WEAVIATE_GRPC_HOST=weaviate
CHUNKER_SERVICE_URL=http://chunker:8003

# Chunker configuration
CHUNKER_EMBEDDING_PROVIDER=voyageai
CHUNKER_VOYAGE_MODEL=voyage-3-lite
CHUNKER_CONCURRENCY=1
CHUNKER_GROUP_SIZE=16
CHUNKER_PROFILE_YAPPI=0

# SQLite state (override if needed)
SEMANTIC_SYNC_DB=backend/data/semantic_sync.db
